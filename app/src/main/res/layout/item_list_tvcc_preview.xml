<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_marginBottom="12dp"
    android:backgroundTint="@color/transparent">

    <TextView
        android:id="@+id/tvTitle"
        style="@style/Theme.IniDesakuBeta.TextView.Regular12.400"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@id/tvMulai"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Produk ikan" />

    <TextView
        android:id="@+id/tvMulai"
        style="@style/Theme.IniDesakuBeta.TextView.Regular12.400"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@id/tvAkhir"
        app:layout_constraintTop_toTopOf="@id/tvTitle"
        tools:text="13.00 - " />

    <TextView
        android:id="@+id/tvAkhir"
        style="@style/Theme.IniDesakuBeta.TextView.Regular12.400"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/tvMulai"
        tools:text="14.00" />

</androidx.constraintlayout.widget.ConstraintLayout>